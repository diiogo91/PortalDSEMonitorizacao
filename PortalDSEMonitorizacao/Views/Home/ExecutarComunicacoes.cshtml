@{
    ViewBag.Title = "ScriptComunicacoes";
}
<h2 align="center">Script -> Report do Estado das Cominicações com os Balcões</h2>
<script src="~/Scripts/jquery-1.9.1.js"></script>
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <title></title>
</head>
<body>
@model PortalDSEMonitorizacao.Models.PowershellModel
    <form id="form1" runat="server">
        <div>
            <table>
                <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                <tr><td>&nbsp;</td><td><h3>Resultado da Execução</h3></td></tr>
                <tr>
                    <td>
                        <br />
                    </td>
                    <td>
                        <asp:TextBox ID="Input" runat="server" TextMode="MultiLine" Width="433px" Height="73px"> @Model.Output</asp:TextBox>
                    </td>
                </tr>
                <tr>
                    <td>
                        &nbsp;
                    </td>
                    <td>
                        <asp:Button ID="ExecuteCode" runat="server" Text="Execute" Width="200" onclick="ExecuteCode_Click" />
                    </td>
                </tr>
                <tr>
                    <td>&nbsp;</td>
                    <td>
                        <h3>Erros da Execução</h3>
                    </td>
                </tr>
                <tr>
                    <td>
                        &nbsp;
                    </td>
                    <td>
                        <asp:TextBox ID="ResultBox" TextMode="MultiLine" Width="700" Height="200" runat="server">@Model.Error</asp:TextBox>
                    </td>
                </tr>
            </table>
        </div>
        <div class="row" Style="margin-top: 1cm;">
            <div class="col-md-1">
                <input type="submit" id="btnexecutar" class="btn btn-success"
                       value="Executar Script" formaction="/home/ExecutarComunicacoes2" />
            </div>
        </div>
    </form>
</body>
</html>